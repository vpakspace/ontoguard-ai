<?xml version="1.0"?>
<!--
    Finance Ontology for OntoGuard
    
    This ontology defines the semantic structure and business rules for a banking
    and financial services system. It includes accounts, transactions, compliance
    rules, and regulatory requirements.
    
    Key Business Rules:
    - Large transactions require manager approval
    - International transfers have additional compliance checks
    - Account closures require multiple approvals
    - Suspicious activity triggers alerts
    - Regulatory limits must be enforced
    - Customer data access is restricted
    
    Namespace: http://example.org/finance#
-->

<rdf:RDF xmlns:rdf="http://www.w3.org/1999/02/22-rdf-syntax-ns#"
         xmlns:owl="http://www.w3.org/2002/07/owl#"
         xmlns:rdfs="http://www.w3.org/2000/01/rdf-schema#"
         xmlns:xsd="http://www.w3.org/2001/XMLSchema#"
         xml:base="http://example.org/finance#">

    <!-- ============================================ -->
    <!-- ONTOLOGY METADATA -->
    <!-- ============================================ -->
    
    <owl:Ontology rdf:about="http://example.org/finance">
        <rdfs:comment>Finance Ontology for OntoGuard Semantic Validation</rdfs:comment>
        <rdfs:label>Finance Ontology</rdfs:label>
        <owl:versionInfo>1.0</owl:versionInfo>
    </owl:Ontology>

    <!-- ============================================ -->
    <!-- CLASSES (CONCEPTS) -->
    <!-- ============================================ -->
    
    <!-- User Roles -->
    <owl:Class rdf:about="#BankEmployee">
        <rdfs:label>Bank Employee</rdfs:label>
        <rdfs:comment>Base class for all bank employees</rdfs:comment>
    </owl:Class>
    
    <owl:Class rdf:about="#Teller">
        <rdfs:subClassOf rdf:resource="#BankEmployee"/>
        <rdfs:label>Teller</rdfs:label>
        <rdfs:comment>Bank teller with limited transaction authority</rdfs:comment>
    </owl:Class>
    
    <owl:Class rdf:about="#Manager">
        <rdfs:subClassOf rdf:resource="#BankEmployee"/>
        <rdfs:label>Manager</rdfs:label>
        <rdfs:comment>Bank manager with elevated authority</rdfs:comment>
    </owl:Class>
    
    <owl:Class rdf:about="#ComplianceOfficer">
        <rdfs:subClassOf rdf:resource="#BankEmployee"/>
        <rdfs:label>Compliance Officer</rdfs:label>
        <rdfs:comment>Compliance officer for regulatory oversight</rdfs:comment>
    </owl:Class>
    
    <owl:Class rdf:about="#Customer">
        <rdfs:label>Customer</rdfs:label>
        <rdfs:comment>Bank customer</rdfs:comment>
    </owl:Class>
    
    <!-- Account and Transaction Classes -->
    <owl:Class rdf:about="#Account">
        <rdfs:label>Account</rdfs:label>
        <rdfs:comment>Bank account (checking, savings, etc.)</rdfs:comment>
    </owl:Class>
    
    <owl:Class rdf:about="#Transaction">
        <rdfs:label>Transaction</rdfs:label>
        <rdfs:comment>Financial transaction</rdfs:comment>
    </owl:Class>
    
    <owl:Class rdf:about="#LargeTransaction">
        <rdfs:subClassOf rdf:resource="#Transaction"/>
        <rdfs:label>Large Transaction</rdfs:label>
        <rdfs:comment>Transaction exceeding $10,000 requiring special handling</rdfs:comment>
    </owl:Class>
    
    <owl:Class rdf:about="#InternationalTransfer">
        <rdfs:subClassOf rdf:resource="#Transaction"/>
        <rdfs:label>International Transfer</rdfs:label>
        <rdfs:comment>Cross-border money transfer requiring compliance checks</rdfs:comment>
    </owl:Class>
    
    <owl:Class rdf:about="#WireTransfer">
        <rdfs:subClassOf rdf:resource="#Transaction"/>
        <rdfs:label>Wire Transfer</rdfs:label>
        <rdfs:comment>Wire transfer transaction</rdfs:comment>
    </owl:Class>
    
    <!-- Compliance and Regulatory -->
    <owl:Class rdf:about="#SuspiciousActivity">
        <rdfs:label>Suspicious Activity</rdfs:label>
        <rdfs:comment>Transaction flagged as potentially suspicious (SAR filing required)</rdfs:comment>
    </owl:Class>
    
    <owl:Class rdf:about="#CustomerData">
        <rdfs:label>Customer Data</rdfs:label>
        <rdfs:comment>Customer personal and financial information</rdfs:comment>
    </owl:Class>
    
    <!-- Action Classes -->
    <owl:Class rdf:about="#Action">
        <rdfs:label>Action</rdfs:label>
        <rdfs:comment>Base class for all actions in the banking system</rdfs:comment>
    </owl:Class>
    
    <owl:Class rdf:about="#ProcessTransaction">
        <rdfs:subClassOf rdf:resource="#Action"/>
        <rdfs:label>Process Transaction</rdfs:label>
        <rdfs:comment>Action to process a financial transaction</rdfs:comment>
    </owl:Class>
    
    <owl:Class rdf:about="#ApproveLargeTransaction">
        <rdfs:subClassOf rdf:resource="#Action"/>
        <rdfs:label>Approve Large Transaction</rdfs:label>
        <rdfs:comment>Action to approve a large transaction</rdfs:comment>
    </owl:Class>
    
    <owl:Class rdf:about="#CloseAccount">
        <rdfs:subClassOf rdf:resource="#Action"/>
        <rdfs:label>Close Account</rdfs:label>
        <rdfs:comment>Action to close a bank account</rdfs:comment>
    </owl:Class>
    
    <owl:Class rdf:about="#AccessCustomerData">
        <rdfs:subClassOf rdf:resource="#Action"/>
        <rdfs:label>Access Customer Data</rdfs:label>
        <rdfs:comment>Action to access customer personal information</rdfs:comment>
    </owl:Class>
    
    <owl:Class rdf:about="#FileSAR">
        <rdfs:subClassOf rdf:resource="#Action"/>
        <rdfs:label>File Suspicious Activity Report</rdfs:label>
        <rdfs:comment>Action to file a Suspicious Activity Report with regulators</rdfs:comment>
    </owl:Class>
    
    <owl:Class rdf:about="#ModifyAccount">
        <rdfs:subClassOf rdf:resource="#Action"/>
        <rdfs:label>Modify Account</rdfs:label>
        <rdfs:comment>Action to modify account information</rdfs:comment>
    </owl:Class>
    
    <owl:Class rdf:about="#CreateAccount">
        <rdfs:subClassOf rdf:resource="#Action"/>
        <rdfs:label>Create Account</rdfs:label>
        <rdfs:comment>Action to create a new bank account</rdfs:comment>
    </owl:Class>
    
    <owl:Class rdf:about="#ProcessInternationalTransfer">
        <rdfs:subClassOf rdf:resource="#Action"/>
        <rdfs:label>Process International Transfer</rdfs:label>
        <rdfs:comment>Action to process an international money transfer</rdfs:comment>
    </owl:Class>

    <!-- ============================================ -->
    <!-- PROPERTIES (RELATIONSHIPS) -->
    <!-- ============================================ -->
    
    <!-- Account Relationships -->
    <owl:ObjectProperty rdf:about="#hasAccount">
        <rdfs:label>has account</rdfs:label>
        <rdfs:comment>Relates a customer to their bank accounts</rdfs:comment>
        <rdfs:domain rdf:resource="#Customer"/>
        <rdfs:range rdf:resource="#Account"/>
    </owl:ObjectProperty>
    
    <!-- Transaction Relationships -->
    <owl:ObjectProperty rdf:about="#hasTransaction">
        <rdfs:label>has transaction</rdfs:label>
        <rdfs:comment>Relates an account to its transactions</rdfs:comment>
        <rdfs:domain rdf:resource="#Account"/>
        <rdfs:range rdf:resource="#Transaction"/>
    </owl:ObjectProperty>
    
    <!-- Action Permission Properties -->
    <owl:ObjectProperty rdf:about="#requiresRole">
        <rdfs:label>requires role</rdfs:label>
        <rdfs:comment>Specifies which role is required to perform an action</rdfs:comment>
        <rdfs:domain rdf:resource="#Action"/>
        <rdfs:range rdf:resource="#BankEmployee"/>
    </owl:ObjectProperty>
    
    <owl:ObjectProperty rdf:about="#requiresApproval">
        <rdfs:label>requires approval</rdfs:label>
        <rdfs:comment>Indicates that an action requires approval from a specific role</rdfs:comment>
        <rdfs:domain rdf:resource="#Action"/>
        <rdfs:range rdf:resource="#BankEmployee"/>
    </owl:ObjectProperty>
    
    <owl:ObjectProperty rdf:about="#appliesTo">
        <rdfs:label>applies to</rdfs:label>
        <rdfs:comment>Specifies which entity type an action can be applied to</rdfs:comment>
        <rdfs:domain rdf:resource="#Action"/>
        <rdfs:range rdf:resource="http://www.w3.org/2002/07/owl#Thing"/>
    </owl:ObjectProperty>
    
    <!-- Data Properties -->
    <owl:DatatypeProperty rdf:about="#transactionAmount">
        <rdfs:label>transaction amount</rdfs:label>
        <rdfs:comment>Amount of money in a transaction</rdfs:comment>
        <rdfs:domain rdf:resource="#Transaction"/>
        <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#decimal"/>
    </owl:DatatypeProperty>
    
    <owl:DatatypeProperty rdf:about="#isSuspicious">
        <rdfs:label>is suspicious</rdfs:label>
        <rdfs:comment>Indicates if a transaction is flagged as suspicious</rdfs:comment>
        <rdfs:domain rdf:resource="#Transaction"/>
        <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#boolean"/>
    </owl:DatatypeProperty>
    
    <owl:DatatypeProperty rdf:about="#requiresKYC">
        <rdfs:label>requires KYC</rdfs:label>
        <rdfs:comment>Indicates if Know Your Customer verification is required</rdfs:comment>
        <rdfs:domain rdf:resource="#Transaction"/>
        <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#boolean"/>
    </owl:DatatypeProperty>
    
    <owl:DatatypeProperty rdf:about="#isInternational">
        <rdfs:label>is international</rdfs:label>
        <rdfs:comment>Indicates if transaction is cross-border</rdfs:comment>
        <rdfs:domain rdf:resource="#Transaction"/>
        <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#boolean"/>
    </owl:DatatypeProperty>

    <!-- ============================================ -->
    <!-- ACTION INDIVIDUALS -->
    <!-- ============================================ -->
    
    <ProcessTransaction rdf:about="#processTransactionAction">
        <rdfs:label>process transaction</rdfs:label>
        <requiresRole rdf:resource="#Teller"/>
        <appliesTo rdf:resource="#Transaction"/>
    </ProcessTransaction>
    
    <ApproveLargeTransaction rdf:about="#approveLargeTransactionAction">
        <rdfs:label>approve large transaction</rdfs:label>
        <requiresRole rdf:resource="#Manager"/>
        <appliesTo rdf:resource="#LargeTransaction"/>
    </ApproveLargeTransaction>
    
    <CloseAccount rdf:about="#closeAccountAction">
        <rdfs:label>close account</rdfs:label>
        <requiresRole rdf:resource="#Manager"/>
        <requiresApproval rdf:resource="#Manager"/>
        <appliesTo rdf:resource="#Account"/>
    </CloseAccount>
    
    <AccessCustomerData rdf:about="#accessCustomerDataAction">
        <rdfs:label>access customer data</rdfs:label>
        <requiresRole rdf:resource="#BankEmployee"/>
        <appliesTo rdf:resource="#CustomerData"/>
    </AccessCustomerData>
    
    <FileSAR rdf:about="#fileSARAction">
        <rdfs:label>file suspicious activity report</rdfs:label>
        <requiresRole rdf:resource="#ComplianceOfficer"/>
        <appliesTo rdf:resource="#SuspiciousActivity"/>
    </FileSAR>
    
    <ModifyAccount rdf:about="#modifyAccountAction">
        <rdfs:label>modify account</rdfs:label>
        <requiresRole rdf:resource="#Manager"/>
        <appliesTo rdf:resource="#Account"/>
    </ModifyAccount>
    
    <CreateAccount rdf:about="#createAccountAction">
        <rdfs:label>create account</rdfs:label>
        <requiresRole rdf:resource="#Teller"/>
        <appliesTo rdf:resource="#Account"/>
    </CreateAccount>
    
    <ProcessInternationalTransfer rdf:about="#processInternationalTransferAction">
        <rdfs:label>process international transfer</rdfs:label>
        <requiresRole rdf:resource="#Manager"/>
        <requiresApproval rdf:resource="#ComplianceOfficer"/>
        <appliesTo rdf:resource="#InternationalTransfer"/>
    </ProcessInternationalTransfer>

    <!-- ============================================ -->
    <!-- BUSINESS RULES AS OWL RESTRICTIONS -->
    <!-- ============================================ -->
    
    <!-- Rule 1: Transactions over $10,000 require manager approval -->
    <owl:Class rdf:about="#LargeTransactionRequiresManager">
        <rdfs:subClassOf rdf:resource="#ApproveLargeTransaction"/>
        <rdfs:comment>Transactions over $10,000 require manager approval</rdfs:comment>
        <owl:equivalentClass>
            <owl:Class>
                <owl:intersectionOf rdf:parseType="Collection">
                    <rdf:Description rdf:about="#ApproveLargeTransaction"/>
                    <owl:Restriction>
                        <owl:onProperty rdf:resource="#appliesTo"/>
                        <owl:someValuesFrom>
                            <owl:Restriction>
                                <owl:onProperty rdf:resource="#transactionAmount"/>
                                <owl:someValuesFrom>
                                    <rdfs:Datatype>
                                        <owl:onDatatype rdf:resource="http://www.w3.org/2001/XMLSchema#decimal"/>
                                        <owl:withRestrictions rdf:parseType="Collection">
                                            <rdf:Description>
                                                <xsd:minExclusive rdf:datatype="http://www.w3.org/2001/XMLSchema#decimal">10000</xsd:minExclusive>
                                            </rdf:Description>
                                        </owl:withRestrictions>
                                    </rdfs:Datatype>
                                </owl:someValuesFrom>
                            </owl:Restriction>
                        </owl:someValuesFrom>
                    </owl:Restriction>
                    <owl:Restriction>
                        <owl:onProperty rdf:resource="#requiresApproval"/>
                        <owl:hasValue rdf:resource="#Manager"/>
                    </owl:Restriction>
                </owl:intersectionOf>
            </owl:Class>
        </owl:equivalentClass>
    </owl:Class>
    
    <!-- Rule 2: International transfers require compliance officer approval -->
    <owl:Class rdf:about="#InternationalTransferRequiresCompliance">
        <rdfs:subClassOf rdf:resource="#ProcessInternationalTransfer"/>
        <rdfs:comment>International transfers require compliance officer approval</rdfs:comment>
        <owl:equivalentClass>
            <owl:Class>
                <owl:intersectionOf rdf:parseType="Collection">
                    <rdf:Description rdf:about="#ProcessInternationalTransfer"/>
                    <owl:Restriction>
                        <owl:onProperty rdf:resource="#appliesTo"/>
                        <owl:someValuesFrom rdf:resource="#InternationalTransfer"/>
                    </owl:Restriction>
                    <owl:Restriction>
                        <owl:onProperty rdf:resource="#requiresApproval"/>
                        <owl:hasValue rdf:resource="#ComplianceOfficer"/>
                    </owl:Restriction>
                </owl:intersectionOf>
            </owl:Class>
        </owl:equivalentClass>
    </owl:Class>
    
    <!-- Rule 3: Account closure requires dual manager approval -->
    <owl:Class rdf:about="#AccountClosureRequiresDualApproval">
        <rdfs:subClassOf rdf:resource="#CloseAccount"/>
        <rdfs:comment>Account closure requires approval from two managers</rdfs:comment>
        <owl:equivalentClass>
            <owl:Class>
                <owl:intersectionOf rdf:parseType="Collection">
                    <rdf:Description rdf:about="#CloseAccount"/>
                    <owl:Restriction>
                        <owl:onProperty rdf:resource="#requiresApproval"/>
                        <owl:minCardinality rdf:datatype="http://www.w3.org/2001/XMLSchema#nonNegativeInteger">2</owl:minCardinality>
                    </owl:Restriction>
                </owl:intersectionOf>
            </owl:Class>
        </owl:equivalentClass>
    </owl:Class>
    
    <!-- Rule 4: Only Compliance Officers can file SARs -->
    <owl:Class rdf:about="#ComplianceOnlyFileSAR">
        <rdfs:subClassOf rdf:resource="#FileSAR"/>
        <rdfs:comment>Restriction: Only Compliance Officer role can file Suspicious Activity Reports</rdfs:comment>
        <owl:equivalentClass>
            <owl:Class>
                <owl:intersectionOf rdf:parseType="Collection">
                    <rdf:Description rdf:about="#FileSAR"/>
                    <owl:Restriction>
                        <owl:onProperty rdf:resource="#requiresRole"/>
                        <owl:hasValue rdf:resource="#ComplianceOfficer"/>
                    </owl:Restriction>
                </owl:intersectionOf>
            </owl:Class>
        </owl:equivalentClass>
    </owl:Class>
    
    <!-- Rule 5: Suspicious transactions must trigger SAR filing -->
    <owl:Class rdf:about="#SuspiciousTransactionRequiresSAR">
        <rdfs:subClassOf rdf:resource="#ProcessTransaction"/>
        <rdfs:comment>Suspicious transactions must have SAR filed before processing</rdfs:comment>
        <owl:equivalentClass>
            <owl:Class>
                <owl:intersectionOf rdf:parseType="Collection">
                    <rdf:Description rdf:about="#ProcessTransaction"/>
                    <owl:Restriction>
                        <owl:onProperty rdf:resource="#appliesTo"/>
                        <owl:someValuesFrom>
                            <owl:Restriction>
                                <owl:onProperty rdf:resource="#isSuspicious"/>
                                <owl:hasValue rdf:datatype="http://www.w3.org/2001/XMLSchema#boolean">true</owl:hasValue>
                            </owl:Restriction>
                        </owl:someValuesFrom>
                    </owl:Restriction>
                </owl:intersectionOf>
            </owl:Class>
        </owl:equivalentClass>
    </owl:Class>
    
    <!-- Rule 6: International transfers require KYC verification -->
    <owl:Class rdf:about="#InternationalTransferRequiresKYC">
        <rdfs:subClassOf rdf:resource="#ProcessInternationalTransfer"/>
        <rdfs:comment>International transfers require Know Your Customer verification</rdfs:comment>
        <owl:equivalentClass>
            <owl:Class>
                <owl:intersectionOf rdf:parseType="Collection">
                    <rdf:Description rdf:about="#ProcessInternationalTransfer"/>
                    <owl:Restriction>
                        <owl:onProperty rdf:resource="#appliesTo"/>
                        <owl:someValuesFrom>
                            <owl:Restriction>
                                <owl:onProperty rdf:resource="#requiresKYC"/>
                                <owl:hasValue rdf:datatype="http://www.w3.org/2001/XMLSchema#boolean">true</owl:hasValue>
                            </owl:Restriction>
                        </owl:someValuesFrom>
                    </owl:Restriction>
                </owl:intersectionOf>
            </owl:Class>
        </owl:equivalentClass>
    </owl:Class>
    
    <!-- Rule 7: Only Managers can modify accounts -->
    <owl:Class rdf:about="#ManagerOnlyModifyAccount">
        <rdfs:subClassOf rdf:resource="#ModifyAccount"/>
        <rdfs:comment>Restriction: Only Manager role can modify account information</rdfs:comment>
        <owl:equivalentClass>
            <owl:Class>
                <owl:intersectionOf rdf:parseType="Collection">
                    <rdf:Description rdf:about="#ModifyAccount"/>
                    <owl:Restriction>
                        <owl:onProperty rdf:resource="#requiresRole"/>
                        <owl:hasValue rdf:resource="#Manager"/>
                    </owl:Restriction>
                </owl:intersectionOf>
            </owl:Class>
        </owl:equivalentClass>
    </owl:Class>
    
    <!-- Rule 8: Tellers can only process transactions up to $5,000 -->
    <owl:Class rdf:about="#TellerTransactionLimit">
        <rdfs:subClassOf rdf:resource="#ProcessTransaction"/>
        <rdfs:comment>Tellers can only process transactions up to $5,000 without approval</rdfs:comment>
        <owl:equivalentClass>
            <owl:Class>
                <owl:intersectionOf rdf:parseType="Collection">
                    <rdf:Description rdf:about="#ProcessTransaction"/>
                    <owl:Restriction>
                        <owl:onProperty rdf:resource="#requiresRole"/>
                        <owl:hasValue rdf:resource="#Teller"/>
                    </owl:Restriction>
                    <owl:Restriction>
                        <owl:onProperty rdf:resource="#appliesTo"/>
                        <owl:someValuesFrom>
                            <owl:Restriction>
                                <owl:onProperty rdf:resource="#transactionAmount"/>
                                <owl:someValuesFrom>
                                    <rdfs:Datatype>
                                        <owl:onDatatype rdf:resource="http://www.w3.org/2001/XMLSchema#decimal"/>
                                        <owl:withRestrictions rdf:parseType="Collection">
                                            <rdf:Description>
                                                <xsd:maxInclusive rdf:datatype="http://www.w3.org/2001/XMLSchema#decimal">5000</xsd:maxInclusive>
                                            </rdf:Description>
                                        </owl:withRestrictions>
                                    </rdfs:Datatype>
                                </owl:someValuesFrom>
                            </owl:Restriction>
                        </owl:someValuesFrom>
                    </owl:Restriction>
                </owl:intersectionOf>
            </owl:Class>
        </owl:equivalentClass>
    </owl:Class>
    
    <!-- Rule 9: Wire transfers over $3,000 require manager approval -->
    <owl:Class rdf:about="#WireTransferRequiresApproval">
        <rdfs:subClassOf rdf:resource="#ProcessTransaction"/>
        <rdfs:comment>Wire transfers over $3,000 require manager approval</rdfs:comment>
        <owl:equivalentClass>
            <owl:Class>
                <owl:intersectionOf rdf:parseType="Collection">
                    <rdf:Description rdf:about="#ProcessTransaction"/>
                    <owl:Restriction>
                        <owl:onProperty rdf:resource="#appliesTo"/>
                        <owl:someValuesFrom rdf:resource="#WireTransfer"/>
                    </owl:Restriction>
                    <owl:Restriction>
                        <owl:onProperty rdf:resource="#appliesTo"/>
                        <owl:someValuesFrom>
                            <owl:Restriction>
                                <owl:onProperty rdf:resource="#transactionAmount"/>
                                <owl:someValuesFrom>
                                    <rdfs:Datatype>
                                        <owl:onDatatype rdf:resource="http://www.w3.org/2001/XMLSchema#decimal"/>
                                        <owl:withRestrictions rdf:parseType="Collection">
                                            <rdf:Description>
                                                <xsd:minExclusive rdf:datatype="http://www.w3.org/2001/XMLSchema#decimal">3000</xsd:minExclusive>
                                            </rdf:Description>
                                        </owl:withRestrictions>
                                    </rdfs:Datatype>
                                </owl:someValuesFrom>
                            </owl:Restriction>
                        </owl:someValuesFrom>
                    </owl:Restriction>
                    <owl:Restriction>
                        <owl:onProperty rdf:resource="#requiresApproval"/>
                        <owl:hasValue rdf:resource="#Manager"/>
                    </owl:Restriction>
                </owl:intersectionOf>
            </owl:Class>
        </owl:equivalentClass>
    </owl:Class>
    
    <!-- Rule 10: Customer data access requires business justification -->
    <owl:Class rdf:about="#CustomerDataAccessRequiresJustification">
        <rdfs:subClassOf rdf:resource="#AccessCustomerData"/>
        <rdfs:comment>Accessing customer data requires documented business justification</rdfs:comment>
        <owl:equivalentClass>
            <owl:Restriction>
                <owl:onProperty rdf:resource="#appliesTo"/>
                <owl:someValuesFrom rdf:resource="#CustomerData"/>
            </owl:Restriction>
        </owl:equivalentClass>
    </owl:Class>
    
    <!-- Rule 11: Transactions over $25,000 require compliance review -->
    <owl:Class rdf:about="#VeryLargeTransactionRequiresCompliance">
        <rdfs:subClassOf rdf:resource="#ApproveLargeTransaction"/>
        <rdfs:comment>Transactions over $25,000 require compliance officer review</rdfs:comment>
        <owl:equivalentClass>
            <owl:Class>
                <owl:intersectionOf rdf:parseType="Collection">
                    <rdf:Description rdf:about="#ApproveLargeTransaction"/>
                    <owl:Restriction>
                        <owl:onProperty rdf:resource="#appliesTo"/>
                        <owl:someValuesFrom>
                            <owl:Restriction>
                                <owl:onProperty rdf:resource="#transactionAmount"/>
                                <owl:someValuesFrom>
                                    <rdfs:Datatype>
                                        <owl:onDatatype rdf:resource="http://www.w3.org/2001/XMLSchema#decimal"/>
                                        <owl:withRestrictions rdf:parseType="Collection">
                                            <rdf:Description>
                                                <xsd:minExclusive rdf:datatype="http://www.w3.org/2001/XMLSchema#decimal">25000</xsd:minExclusive>
                                            </rdf:Description>
                                        </owl:withRestrictions>
                                    </rdfs:Datatype>
                                </owl:someValuesFrom>
                            </owl:Restriction>
                        </owl:someValuesFrom>
                    </owl:Restriction>
                    <owl:Restriction>
                        <owl:onProperty rdf:resource="#requiresApproval"/>
                        <owl:someValuesFrom rdf:resource="#ComplianceOfficer"/>
                    </owl:Restriction>
                </owl:intersectionOf>
            </owl:Class>
        </owl:equivalentClass>
    </owl:Class>
    
    <!-- Rule 12: Account creation requires KYC for new customers -->
    <owl:Class rdf:about="#NewAccountRequiresKYC">
        <rdfs:subClassOf rdf:resource="#CreateAccount"/>
        <rdfs:comment>Creating accounts for new customers requires KYC verification</rdfs:comment>
        <owl:equivalentClass>
            <owl:Class>
                <owl:intersectionOf rdf:parseType="Collection">
                    <rdf:Description rdf:about="#CreateAccount"/>
                    <owl:Restriction>
                        <owl:onProperty rdf:resource="#appliesTo"/>
                        <owl:someValuesFrom>
                            <owl:Restriction>
                                <owl:onProperty rdf:resource="#requiresKYC"/>
                                <owl:hasValue rdf:datatype="http://www.w3.org/2001/XMLSchema#boolean">true</owl:hasValue>
                            </owl:Restriction>
                        </owl:someValuesFrom>
                    </owl:Restriction>
                </owl:intersectionOf>
            </owl:Class>
        </owl:equivalentClass>
    </owl:Class>

</rdf:RDF>
